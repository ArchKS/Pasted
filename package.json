{
  "name": "electron-example",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --watch . --exec electron .",
    "dist": "electron-builder",
    "package-mac": "electron-packager . clipboard --overwrite --platform=darwin --arch=x64 --icon=icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . clipboard --overwrite --asar --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . clipboard --overwrite --asar --platform=linux --arch=x64 --icon=icon.ico --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "clipboard": "^2.0.8",
    "electron-clipboard-watcher": "^1.0.1",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.3.0",
    "electron-store": "^8.0.0",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0"
  },
  "build": {
    "appId": "org.zulip.zulip-electron",
    "category": "Chat;GNOME;GTK;Network;InstantMessaging",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "assets/icon.ico"
    }
  }
}